    "bootstrap": "^4.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-recommended-bump": "^6.0.9",
    "copy-webpack-plugin": "^6.0.2",
